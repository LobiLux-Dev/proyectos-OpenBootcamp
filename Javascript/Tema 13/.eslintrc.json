{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "quotes": ["error", "double"],
        "max-len": ["warn", 140],
        "semi": ["error", "never"],
        "no-unused-vars": ["off"]
    }
}
